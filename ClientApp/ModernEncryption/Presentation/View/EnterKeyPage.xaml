<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ModernEncryption.Presentation.View.EnterKeyPage">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name ="EnterKeyPlaceholder" Placeholder ="Please enter your key." />
            <Button Text="Submit" TextColor="White"  BorderRadius="15" BackgroundColor="#3A99D8" HorizontalOptions="Center"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>